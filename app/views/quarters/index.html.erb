<div class="page-header">
  <h1>
    Quarters
    <a href="/quarters/new" class="btn btn-lg btn-success">New Quarter</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @quarters.each do |quarter| %>
      <tr>
        <td>
          <a href="/quarters/<%= quarter.id %>" class="btn btn-primary">Show</a>
          <a href="/quarters/<%= quarter.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_quarter/<%= quarter.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

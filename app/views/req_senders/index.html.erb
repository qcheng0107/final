<div class="page-header">
  <h1>
    Req senders
    <a href="/req_senders/new" class="btn btn-lg btn-success">New Req sender</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Student</th>
        <th>Actions</th>
      </tr>

      <% @req_senders.each do |req_sender| %>
      <tr>
        <td><%= req_sender.student.email %></td>
        <td>
          <a href="/req_senders/<%= req_sender.id %>" class="btn btn-primary">Show</a>
          <a href="/req_senders/<%= req_sender.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_req_sender/<%= req_sender.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

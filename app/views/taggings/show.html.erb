<div class="page-header">
  <h3>Tagging #<%= @tagging.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Skill</dt>
      <dd>
        <% if @tagging.skill.present? %>
          <a href="/skills/<%= @tagging.skill_id %>">
            <%= @tagging.skill.student_id %>
          </a>
        <% end %>
      </dd>

      <dt>Student</dt>
      <dd>
        <% if @tagging.student.present? %>
          <a href="/students/<%= @tagging.student_id %>">
            <%= @tagging.student.email %>
          </a>
        <% end %>
      </dd>

      <dt>Team</dt>
      <dd>
        <% if @tagging.team.present? %>
          <a href="/teams/<%= @tagging.team_id %>">
            <%= @tagging.team.course_id %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/taggings" class="btn btn-primary">
        Back
      </a>
      <a href="/taggings/<%= @tagging.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_tagging/<%= @tagging.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
